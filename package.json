    "@types/react-dom": "18.0.11",
    "@types/react-modal": "3.13.1",
    "@types/sanitize-html": "2.8.0",
    "@types/walk": "2.3.1",
    "aoe_technology_radar": "3.5.0",
    "classnames": "2.3.2",
    "d3": "7.8.2",
    "front-matter": "4.0.2",
    "fs-extra": "11.1.0",
    "highlight.js": "11.7.0",
    "husky": "8.0.3",
    "marked": "4.2.12",
    "moment": "2.29.4",
    "node-fetch": "3.3.1",
    "postcss-normalize": "10.0.1",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-modal": "3.16.1",
    "react-router-dom": "6.8.2",
    "react-scripts": "5.0.1",
    "react-tooltip": "4.5.1",
    "sanitize-html": "2.10.0",
    "sass": "1.58.3",
    "typescript": "4.9.5",
    "walk": "2.3.15",
    "xml-sitemap": "^0.3.2",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/marked": "4.0.8",
    "@types/node": "18.14.2",
    "@typescript-eslint/parser": "5.54.0",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "yallist": "4.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
